  "bugs": {
    "url": "https://github.com/cxntered/hypixel-ban-tracker/issues"
  },
  "homepage": "https://github.com/cxntered/hypixel-ban-tracker#readme",
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "discord.js": "^14.9.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  }
